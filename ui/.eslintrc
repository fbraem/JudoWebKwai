{
    "extends" : [
        "strongloop",
        "plugin:vue/essential"
    ],
    "plugins" : [
        "vue"
    ],
    "parserOptions" : {
        "parser" : "babel-eslint",
    },
    "env" : {
        "browser" : true,
        "es6" : true
    },
    "rules" : {
        "comma-dangle" : [
            "error",
            {
                "functions" : "never",
                "objects" : "only-multiline",
                "arrays" : "always-multiline"
            }
        ]
    }
}
